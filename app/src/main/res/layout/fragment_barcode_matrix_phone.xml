<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".presentation.views.fragments.barcodeAnalysis.defaultBarcode.matrix.BarcodeMatrixPhoneFragment">

    <TextView
        android:id="@+id/fragment_barcode_matrix_phone_number_label"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/matrix_phone_tel_number_label"
        style="@style/AppTheme.TextView.Appearance.Title.Variant" />

    <TextView
        android:id="@+id/fragment_barcode_matrix_phone_number_text_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/fragment_barcode_matrix_phone_number_label"
        android:textIsSelectable="true"
        style="@style/AppTheme.TextView.Appearance.Standard.Secondary.Italic" />

</RelativeLayout>